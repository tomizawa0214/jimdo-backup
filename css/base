<script type="text/javascript">
//<![CDATA[
    $(document).ready(function() {
        // スマホ用ヘッダーにアパート情報追加
        // ヘッダー「住まいのご相談」文言変更
        if ($(window).width() < 768) {
            $('header').prepend('<a class="apart-sp" href="https://www.nemoto-k.net/apartment/">賃貸物件<\/a>');
            $('#cc-nav-view-2470087090 a').text('住まいのご相談');
        }
        $(window).resize(() => {
            if ($(window).width() < 768 && !$('a').hasClass('apart-sp')) {
                $('header').prepend('<a class="apart-sp" href="https://www.nemoto-k.net/apartment/">賃貸物件<\/a>');
            } else if ($(window).width() > 767 && $('a').hasClass('apart-sp')) {
                $('.apart-sp').remove();
            }
            if ($(window).width() < 768) {
                $('#cc-nav-view-2470087090 a').text('住まいのご相談');
            } else {
                $('#cc-nav-view-2470087090 a').text('住まいのご相談 ➟');
            }
        });
        
        // お問い合わせの背景
        $('#content_area > div:nth-of-type(2) > div:nth-last-of-type(5), #content_area > div:nth-of-type(2) > div:nth-last-of-type(4)').wrapAll('<div class="contact-bg"><\/div>');
        
        // お問い合わせのカラム隙間削除
        $('.contact-bg > div:nth-of-type(2) > div:nth-of-type(1) > div:nth-of-type(1)').parent().css({'padding': '0', 'width': '50%' });
        $('.contact-bg > div:nth-of-type(2) > div:nth-of-type(3) > div:nth-of-type(1)').parent().css({'width': '50%', 'height': '100%' });
        $('.contact-bg > div:nth-of-type(2) > div:nth-of-type(3) > div:nth-of-type(1) .j-module.n.j-googlemaps').children().css('height', '332px');
        $('.contact-bg > div:nth-of-type(2) > div:nth-of-type(3) > div:nth-of-type(1) iframe').css('height', '332px');
        
        // お客様の声カラム隙間
        $('#content_area > div:nth-of-type(2) > div:nth-last-of-type(2) > div:nth-of-type(1) > div:nth-of-type(1), #content_area > div:nth-of-type(2) > div:nth-last-of-type(2) > div:nth-of-type(3) > div:nth-of-type(1), #cc-matrix-3857220190, #content_area > div:nth-of-type(2) > div:nth-last-of-type(1) > div:nth-of-type(1) > div:nth-of-type(1), #content_area > div:nth-of-type(2) > div:nth-last-of-type(1) > div:nth-of-type(3) > div:nth-of-type(1)').parent().css({'width': 'auto', 'padding-right': '30px'});
        $('#content_area > div:nth-of-type(2) > div:nth-last-of-type(2) > div:nth-of-type(5) > div:nth-of-type(1), #content_area > div:nth-of-type(2) > div:nth-last-of-type(1) > div:nth-of-type(5) > div:nth-of-type(1)').parent().css({'width': 'auto', 'float': 'left'});
        
        // footer作成
        $('.jtpl-main.cc-content-parent').append('<footer><\/footer>');
        $('.jtpl-footer.footer-options').appendTo('footer');
        
        $('.jtpl-sidebar.sidebar-options').remove();
        
        // // スマホメニューバー対応
        // const setFillHeight = () => {
        //     let vh = $(window).innerHeight() * 0.01;
        //     $('html').css('--vh', `${vh}px`);
        // }
        // $(window).resize(setFillHeight);
        // setFillHeight();
    });
    
    // スマホメニューバー対応
    const setFillHeight = () => {
      let vh = window.innerHeight * 0.01;
      document.documentElement.style.setProperty('--vh', `${vh}px`);
    }
    window.addEventListener('resize', setFillHeight);
    setFillHeight();
//]]>
</script>